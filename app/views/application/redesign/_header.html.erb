<%= tag.header aria: { label: "header" } do %>
  <div class="">
    <%= link_to root_path do %>
      <div>
        <%= inline_svg_tag "joy-logo.svg", class: "fill-current", style: "max-width: 32px;" %>
      </div>
      <span class>Joy of Rails</span>
    <% end %>
    <%= turbo_frame_tag :header_navigation, src: users_header_navigation_path, target: :_top do %>
      <%= render partial: "application/header/nav", layout: "layouts/header/nav" %>
    <% end %>
  </div>
<% end %>
