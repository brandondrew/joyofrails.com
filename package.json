{
  "name": "app",
  "private": "true",
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.0-beta.1",
    "autoprefixer": "^10.4.16",
    "esbuild": "^0.19.8",
    "postcss": "^8.4.31",
    "sass": "^1.69.5",
    "system-font-css": "^2.0.2",
    "tailwindcss": "^3.3.5",
    "vite": "^5.0.0",
    "vite-plugin-rails": "^0.5.0",
    "vite-plugin-ruby": "^5.0.0"
  },
  "scripts": {
    "build": "bin/vite build",
    "esbuild": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=/assets",
    "esbuild:css": "sass ./app/assets/stylesheets/application.sass.scss:./app/assets/builds/application.css --no-source-map --load-path=node_modules"
  }
}
